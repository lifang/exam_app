<script type="text/javascript" src="/javascripts/plugin_assets/My97DatePicker/WdatePicker.js" ></script>
<div id="base_info_div">
  <%= render :partial => "exam_base_info" %>
</div>
<div class="clear">&nbsp;</div>
<div class="main_box_back">
  <h2><span><a onclick="javascript:exam_user_for_now(<%= @examination.id %>)">考生信息</a>
      &nbsp;&nbsp;|&nbsp;&nbsp;<%= link_to_remote "批量录入考生信息", :url => "/exam_user/exam_users/#{@examination.id}/pile_exam_users",
        :method => :get,:update => "exam_user_list" %></span>

    <div class="right">
      <span class="expForEmail"><span id="nameErr"></span></span>&nbsp;&nbsp;&nbsp;
      <% if @examination.user_affirm == true %>
        <a href="/examinations/<%= @examination.id %>/export_user_unaffirm" target="_blank">[导出未确认的考生名单]</a>
      <% end %>
      <a href="/examinations/<%= @examination.id %>/exam_result">[查看成绩]</a>
    </div>
  </h2>
  <div class="in" id="exam_user_list">
    <%= render :partial => "exam_user_for_now"  %>
  </div>
</div>
<div class="clear">&nbsp;</div>
<div class="main_box_back">
  <h2><span>阅卷老师信息</span></h2>
  <div class="in" id="exam_rater_list">
    <%= render :partial => "exam_rater" %>
  </div>
</div>