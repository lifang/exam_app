
<div class="main_box_back">
  <h2><span>试卷列表</span><div class="right"><a href="#">新建试卷</a></div></h2>
  <div class="in">
    <table>
      <colgroup>
        <col width="140" />
        <col width="100" />
        <col width="80" />
        <col width="140" />
        <col width="180" />
        <col width="" />
      </colgroup>
      <thead>
        <tr>
          <th>试卷编号</th>
          <th>标题</th>
          <th>创建日期</th>
          <th>试题数</th>
          <th>备注</th>
          <th></th>
        </tr>
      </thead>
        <% @papers.each do |paper| %>
        <tr>
          <td><%= paper.id  %></td>
          <td>  <%= paper.title  %></td>
          <td> <%= paper.created_at  %></td>
          <td> <%= paper.total_question_num %></td>
          <td> <%= paper.description  %></td>
            <td> <%= paper.types  %></td>
        </tr>
      <% end  %>
      <tbody>
        <tr>
          <td>jenson</td>
          <td>2010-09-08</td>
          <td>5</td>
          <td>梧桐花园</td>
          <td></td>
          <td><a href="#">查看&nbsp;|&nbsp;编辑</a></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
