<div class="products_list_style_box" id="question_info_<%= question_info.attributes["id"].to_i %>" style="display:none">
  <div class="arrow" style="top:35px;"></div>
  <div class="big_pic_box" style="top:0px;">
    <div class="in">
      <div class="title"><%= question_info.elements["title"].text.to_s.html_safe %></div>
      <% question_info.elements["questions"].each_element do |question| %>
        <%= render :partial => "/common/show_single_question", :object => question %>
      <% end %>
    </div>
  </div>
</div>
